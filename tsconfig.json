{
    "compilerOptions": {
        "target": "es5",
        "downlevelIteration": true,
        "jsx": "react-jsx",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "lib": ["dom", "es5", "es2015", "es2016", "es2017.object" , "ES2019.Array"],
        "importHelpers": true,
        "experimentalDecorators": true,
        "baseUrl": ".",
        "paths": {
            "*": [
                "modules/*",
                "node_modules/*"
            ]
        },
    },
    "include": [
        "src/script.tsx"
    ]
  }